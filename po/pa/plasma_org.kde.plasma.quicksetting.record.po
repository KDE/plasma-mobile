msgid "Record Screen"
msgstr "ਸਕਰੀਨ ਰਿਕਾਰਡ ਕਰੋ"

#: package/contents/ui/main.qml:19
#, kde-format
msgid "Recording…"
msgstr "ਰਿਕਾਡਿੰਗ…"

#: package/contents/ui/main.qml:21
#, kde-format
msgid "Writing…"
msgstr "…ਲਿਖਿਆ ਜਾ ਰਿਹਾ ਹੈ"

#: package/contents/ui/main.qml:25
#, kde-format
